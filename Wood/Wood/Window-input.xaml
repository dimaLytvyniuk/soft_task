<Window x:Class="Wood.Window_input"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Wood"
        mc:Ignorable="d"
        Title="Window_input" Height="300" Width="400">
    <Grid>
        <Button x:Name="button" Content="Начать работу" HorizontalAlignment="Left" Margin="80,202,0,0" VerticalAlignment="Top" Width="240" Height="43" Background="#FFEEDD82" FontFamily="Yu Gothic UI Semilight" FontSize="16" Click="button_Click"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="27" Margin="60,78,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="278" BorderThickness="3" BorderBrush="#FFEEDD82" FontSize="14" FontFamily="Yu Gothic Medium"/>

    </Grid>
</Window>
